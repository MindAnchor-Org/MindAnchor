{
  "manifest_version": 3,
  "name": "MindAnchor Extension",
  "description": "MindAnchor Extension designed for ADHD",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "MindAnchor Extension"
  },
  "icons": {
    "16": "icon.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": ["*://*/*"]
}